iters ::= import("dash:iter")
range ::= iters.range

fn sieve(n: i32) {
  multiples := array()
  primes := array()

  for i in 2..n+1 {
    if !multiples.has(i) {
      primes = primes.add(i)
      for j in range(i**2, n + 1, i) {
        multiples = multiples.add(j)
      }
    }
  }

  primes
}

for i in sieve(100)
  write(i, "\n");